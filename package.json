{
  "name": "vite-plugin-stylex-dev",
  "version": "0.5.2",
  "description": "an unofficial stylex vite plugin",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "packageManager": "yarn@3.6.3",
  "workspaces": [
    "examples/*",
    "e2e/**/*",
    "internal/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./client.d.ts"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "client.d.ts"
  ],
  "scripts": {
    "dev": "rollup --config rollup.config.ts --configPlugin swc3 --watch",
    "build": "rollup --config rollup.config.ts --configPlugin swc3",
    "test": "c8 -r=lcov ava __tests__/*.spec.ts",
    "e2e": "ava e2e/e2e.spec.ts"
  },
  "keywords": [
    "stylex",
    "experimental",
    "css-in-js",
    "vite-plugin"
  ],
  "author": "kanno",
  "license": "MIT",
  "homepage": "https://github.com/nonzzz/vite-plugin-stylex",
  "bugs": {
    "url": "https://github.com/nonzzz/vite-plugin-stylex/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nonzzz/vite-plugin-stylex.git"
  },
  "devDependencies": {
    "@stylex-extend/core": "^0.2.1",
    "@stylexjs/stylex": "^0.5.1",
    "@swc-node/register": "^1.9.0",
    "@swc/core": "^1.4.16",
    "@types/babel__core": "^7.20.5",
    "@types/connect": "^3.4.38",
    "@types/node": "^20.10.4",
    "@types/postcss-pxtorem": "^6.0.3",
    "@typescript-eslint/parser": "^6.14.0",
    "@typescript-eslint/utils": "^6.14.0",
    "ava": "^5.3.1",
    "c8": "^8.0.1",
    "eslint": "^8.55.0",
    "eslint-config-kagura": "^2.2.1",
    "eslint-plugin-vue": "^9.19.2",
    "get-tsconfig": "^4.7.2",
    "lightningcss": "^1.24.1",
    "playwright": "~1.32.3",
    "postcss-pxtorem": "^6.0.0",
    "rollup": "^4.16.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-swc3": "^0.11.1",
    "typescript": "^5.3.3",
    "vite": "^^5.2.9",
    "vite-plugin-inspect": "^0.8.4",
    "vite-tsconfig-paths": "^4.3.2",
    "vue-eslint-parser": "^9.3.2"
  },
  "dependencies": {
    "@babel/core": "^7.23.9",
    "@rollup/pluginutils": "^5.1.0",
    "@stylex-extend/babel-plugin": "^0.2.1",
    "@stylexjs/babel-plugin": "^0.6.1",
    "magic-string": "^0.30.5"
  },
  "resolutions": {
    "sharp": "0.32.6",
    "vite": "^5.2.9",
    "@stylexjs/stylex": "^0.6.1"
  },
  "ava": {
    "files": [
      "__tests__/*.spec.ts",
      "e2e/*.spec.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "@swc-node/register"
    ]
  }
}
