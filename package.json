{
  "name": "vite-plugin-stylex-dev",
  "version": "0.1.3",
  "description": "an unofficial stylex vite plugin",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "packageManager": "yarn@3.6.3",
  "workspaces": [
    "examples/*",
    "e2e/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./url.js": {
      "types": "./dist/url.d.ts",
      "import": "./dist/url.mjs",
      "require": "./dist/url.js"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsup --watch src",
    "build": "tsup",
    "test": "c8 -r=lcov ava __tests__/*.spec.ts",
    "e2e": "ava e2e/e2e.spec.ts"
  },
  "keywords": [
    "stylex",
    "experimental",
    "css-in-js"
  ],
  "author": "kanno",
  "license": "MIT",
  "devDependencies": {
    "@stylexjs/stylex": "^0.4.1",
    "@types/babel__core": "^7.20.5",
    "@types/connect": "^3.4.38",
    "@types/node": "^20.10.4",
    "@typescript-eslint/parser": "^6.14.0",
    "@typescript-eslint/utils": "^6.14.0",
    "ava": "^5.3.1",
    "c8": "^8.0.1",
    "eslint": "^8.55.0",
    "eslint-config-kagura": "^2.1.1",
    "eslint-plugin-vue": "^9.19.2",
    "playwright": "~1.32.3",
    "postcss-pxtorem": "^6.0.0",
    "tsup": "^8.0.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vue-eslint-parser": "^9.3.2"
  },
  "dependencies": {
    "@babel/core": "^7.23.6",
    "@babel/plugin-syntax-flow": "^7.23.3",
    "@babel/plugin-syntax-jsx": "^7.23.3",
    "@babel/plugin-syntax-typescript": "^7.23.3",
    "@stylexjs/babel-plugin": "^0.4.1",
    "magic-string": "^0.30.5",
    "rs-module-lexer": "^2.1.1"
  },
  "resolutions": {
    "sharp": "0.32.6"
  },
  "ava": {
    "files": [
      "__tests__/*.spec.ts",
      "e2e/*.spec.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "tsx/cjs"
    ]
  }
}
